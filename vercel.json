{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/server/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/client/$1"
        }
    ],
    "redirects": [
        {
            "source": "/",
            "destination": "/client/index.html",
            "permanent": false
        }
    ]
}